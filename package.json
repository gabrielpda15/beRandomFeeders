{
  "name": "berandomfeeders",
  "version": "1.0.0",
  "engines": {
    "node": "15.4.0",
    "npm": "7.3.0"
  },
  "main": "app.ts",
  "scripts": {
    "start": "nodemon --ignore \"app.js\" --exec \"npx ts-node\" ./src/app.ts",
    "build": "tsc && cd dist && node app.js",
    "heroku-postbuild": "tsc",
    "typeorm": "node -r ts-node/register ./node_modules/typeorm/cli.js",
    "migration:generate": "npm run typeorm -- migration:generate --config src/config/mysql.config.ts --name ",
    "migration:run": "npm run typeorm -- migration:run",
    "migration:create": "npm run typeorm -- migration:create --name ",
    "types": "tsc src/app.js --declaration --allowJs --emitDeclarationOnly --outDir types"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gabrielpda15/beRandomFeeders.git"
  },
  "author": "gabrielpda15",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gabrielpda15/beRandomFeeders/issues"
  },
  "homepage": "https://github.com/gabrielpda15/beRandomFeeders#readme",
  "dependencies": {
    "axios": "^0.21.1",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "glob": "^7.1.6",
    "jsonwebtoken": "^8.5.1",
    "mysql": "^2.18.1",
    "otpauth": "^6.2.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.3",
    "typeorm": "^0.2.29",
    "uuid": "^8.3.2",
    "xmlhttprequest": "^1.8.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.9",
    "@types/crypto-js": "^4.0.1",
    "@types/express": "^4.17.9",
    "@types/glob": "^7.1.3",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.14.19",
    "@types/uuid": "^8.3.0",
    "nodemon": "^2.0.6",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
